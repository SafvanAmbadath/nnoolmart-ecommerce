                    <li><a href="home-shop-1.html">Home</a></li>
                    <li><a href="#">Login / Register</a></li>
                </ul>
            </div> -->

        </div>
    </section>
    
    <!-- LOGIN REGISTER
    ================================================== -->
    <section class="md" data-background="img/bg/bg-1.jpg" style="background-image: url(img/bg/bg-1.jpg);background-size: cover;">
        <div class="container">
            <div class="row">

                <div class="col-lg-6 mb-1-9 mb-lg-0">

                    <div class="common-block">

                        <div class="inner-title">
                           
                            <h4 class="mb-0">Login</h4>
                            <!-- <p class="mb-0">Everything is simple with Login.</p> -->
                        </div>

                        <form action="/adminlogin" method="post">

                            <div class="row">

                                <div class="col-sm-12">

                                    <div class="form-group">
                                        <label>Admin Name</label>
                                        <input type="text" class="form-control" name="name" placeholder="Your name here">
                                    </div>

                                </div>

                                <div class="col-sm-12">

                                    <div class="form-group">
                                        <label>Password </label>
                                        <input type="password" class="form-control" name="password" placeholder="Your password here">
                                    </div>

                                </div>

                            </div>
                            <% if(adminLogErr.length>0) {%>
                                <p class="form-text" id="" style="color:red"><%= adminLogErr %></p>
                                <% } %>

                            <!-- <div class="row">
                                <div class="col-sm-6">
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" value="" id="login-remember">
                                        <label class="form-check-label" for="login-remember">Keep me signed in</label>
                                    </div>
                                </div>
                                <div class="col-sm-6 text-start text-md-end">
                                    <a href="account-password-recovery.html" class="m-link-muted">Forgot password?</a>
                                </div>

                            </div> -->

                            <button type="submit"  class="butn-style2 mt-4">Login</button>

                        </form>

                    </div>

                </div>
            </div> 
        </div> 
    </section>
</div> 

 
                 <!-- all js include start -->

    <!-- jQuery -->
    <script src="js/jquery.min.js"></script>

